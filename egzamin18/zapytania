select nazwa, ilosc, opis, cena, zdjecie from produkty where rodzaje_id IN(1,2)

select produkty.nazwa, producenci.nazwa from produkty join producenci on producenci.id=produkty.Producenci_id where ilosc<10 and rodzaje_id="1";

create user 'anna'@'localhost' identified by '@nna23';

